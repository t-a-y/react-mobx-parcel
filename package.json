{
  "name": "react-mobx-parcel",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html --no-autoinstall",
    "build": "yarn clean && parcel build index.html -d build --no-minify --detailed-report --no-source-maps",
    "clean": "rm -rf .cache dist build"
  },
  "dependencies": {
    "mobx": "^5.10.0",
    "mobx-react": "^6.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass": "^1.20.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "json5": "^2.1.0",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.12.3"
  }
}
